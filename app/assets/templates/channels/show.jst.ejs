
<head>
  <style>

  body{
    padding: 20px;
  }

  .posts > ul{
    float: right;
  }

  .posts > ul > li > a{
    display: block;
    float: left;
  }

  body{
    background: rgb(202, 227, 221);
  }

  nav.sub-show{
    width: 700px;
    height: 60px;
    margin:auto;
    background: rgb(228, 234, 233);
  padding: 5px 10px;
  border: 1px solid lightblue;
  }

  main.posts-sub{
    width:700px;
    min-height:500px;
    background: rgb(241, 246, 245);
    margin:auto;
  padding: 5px 10px;
  border: 1px solid lightblue;
  }

  nav.sub-show >ul{
    float:left;
  }

  nav.sub-show >ul > li {
    float:right;
    padding: 10px 15px;
  }

  nav >h3{

    font-weight:bold;

  }

 main.posts-sub > ul{

  }

  main.posts-sub > ul > li{
    float:right;
    width: 200px;
    margin: 15px;
    height: 200px;
    background: white;
    overflow: hidden;
    box-shadow: 0px 1px 2px #c4c4c4;
  }

  footer.sub-footer{
      width:700px;
      height:60px;
      background: rgb(228, 234, 233);
      margin:auto;
    padding: 5px 10px;
    border: 1px solid lightblue;
  }



  footer > ul{
    float:left;
  }

  footer >ul> li{
    float: right;
    padding: 10px 15px;
  }


  </style>
</head>

<body>
<nav class="sub-show">
 <ul class="group">
   <li><a href="#/posts"><h3>All Posts</h3></a></li>

   <li><a href="#/posts/new"><h3>Create Post</h3></a></li>

   <li><h3><%= channel.escape("title") %></h3></li>
</ul>
</nav>
<main class="posts-sub">
<ul>
  <% posts.each(function(post){ %>


    <% if(post.get("channel_id") === channel.id){ %>

     <li><a href="#/posts/<%= post.id %>"><%= post.escape("title") %><img src="<%= post.get("photo_url") %>"></a>



     </li>
  <% } %>


  <% }); %>
</ul>
</main>

<footer class="sub-footer">
  <ul>
  <% subs.each(function(sub){ %>
     <% if (sub.id === channel.get("sub_id")){ %>
       <li><a href="#/subs/<%= sub.id %>"><%= sub.escape("title") %></a></li>
     <% }  %>

  <% }) %>
  </ul>
</footer>
</body>