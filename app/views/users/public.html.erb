<head>
  <style>
  figure{
    width: 100px;
    height: auto;
  }

  main > .user-info{

    margin:50px;
  }

  main > .user-info > ul > li{
    padding: 10px 15px;
    font-weight: bold;
  }

  </style>

  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-59233651-1', 'auto');
    ga('send', 'pageview');

  </script>
</head>



<body>
<main class="post-article other user-profile">
  <nav class="user">
      <ul class="group">
    <li><%= link_to "Write To Author", new_message_url %></li>
    </ul>
  </nav>

    <div class="user-info">
    <ul class="group">
      <li><h3>Username: <%= @user.username %></h3></li>

      <li><h3>Age: <%=@user.age%></h3></li>

      <li><h3>Gender: <%=@user.gender%></h3></li>


      <li><h3>Zip: <%=@user.zip%></h3></li>
    </ul>

<div class="user-pic">
  <figure class="user">
    <%= image_tag @user.photo.url(:thumb) %>
  </figure>
  </div>
  </div>
</main>



</body>
