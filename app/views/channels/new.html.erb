<form action="<%= sub_channels_url(@sub) %>" method="post">
  <input type="hidden" name="authenticity_token" 
         value="<%= form_authenticity_token %>" />
         
  
     
  <label for="channel_title">Title</label>
  <input
    type="text"
    name="channel[title]"
    value="<%= @channel.title %>"
    id="channel_title">
    <br>
    
  
    
  <input type="submit" value="Create Channel">
     
</form>