<head>
    <script>
    var pics = ["flower", "braid", "garden"];
    var i =0;
      setInterval(function(){

          $(".home").removeClass(pics[i % 3]).addClass(pics[(i+1)% 3]);

          $(".item").html("");
          var word = pics[(i+1)% 3];
          var j = 0;
          setInterval(function(){
              $(".item").append(word[j]);
              j++;
          }, 100);

          i++;

      }, 2000);
    </script>

</head>
<body>
<main class="home">

    <h1 class="make">Let's make </h1><h3 class="item"></h3>









</main>

